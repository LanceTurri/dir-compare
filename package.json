{
  "name": "dir-snap",
  "version": "0.2.3",
  "description": "This compares the contents of a directory against a previous snapshot.",
  "keywords": [
    "directory",
    "compare",
    "folders",
    "snapshot"
  ],
  "main": "./core/snapshot.js",
  "bin": {
    "dirsnap": "./bin/dirsnap"
  },
  "author": "Lance Turri",
  "repository": {
    "type": "git",
    "url": "https://github.com/LanceTurri/dir-snap"
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "debug": "DEBUG=* node --inspect-brk ./bin/dirsnap-reference --folder test/testParent"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "coveralls": "^2.13.1",
    "debug": "^3.0.1",
    "json-diff": "^0.5.1",
    "md5": "^2.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0"
  },
  "preferGlobal": true,
  "license": "MIT"
}
